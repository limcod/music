{
  "name": "music",
  "productName": "music",
  "version": "0.1.0",
  "description": "music",
  "main": "dist/lib/main.js",
  "private": true,
  "author": "youliso@Hotmail.com",
  "license": "MIT",
  "scripts": {
    "retrieval": "npx gulp retrieval",
    "gulp": "npx gulp retrieval && npx gulp compress && npx gulp compiler",
    "start": "electron src/lib/main.js",
    "build": "electron-builder",
    "build w": "electron-builder -w"
  },
  "build": {
    "compression": "maximum",
    "productName": "music",
    "appId": "org.music",
    "copyright": "MIT",
    "asar": true,
    "directories": {
      "output": "out"
    },
    "files": [
      "out/electron/**/*",
      "node_modules/",
      "dist/"
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "resources/icons/icon.ico",
      "uninstallerIcon": "resources/icons/unicon.ico",
      "installerHeaderIcon": "resources/icons/icon.ico",
      "installerSidebar": "resources/icons/modern-wizard.bmp",
      "uninstallerSidebar": "resources/icons/modern-wizard.bmp",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "music",
      "include": "resources/script/installer.nsh"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "resources/icons/icon.icns"
    },
    "win": {
      "icon": "resources/icons/icon.ico",
      "requestedExecutionLevel": "highestAvailable",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "resources/icons"
    }
  },
  "devDependencies": {
    "electron": "^9.2.1",
    "electron-builder": "^22.8.0",
    "google-closure-compiler": "^20200517.0.0",
    "gulp": "^4.0.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-remove-empty-lines": "^0.1.0",
    "gulp-remove-html": "^1.3.0"
  },
  "dependencies": {}
}
